<!doctype html>
<html lang="en" ng-app="app">
<head>
  <meta charset="utf-8">
  <title><%= titulo %></title>
  <link rel="stylesheet" href="css/app.css">
  <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css">
  <script src="//ajax.googleapis.com/ajax/libs/angularjs/1.2.15/angular.min.js"></script>
  <script src="//cdnjs.cloudflare.com/ajax/libs/angular-ui-bootstrap/0.10.0/ui-bootstrap-tpls.min.js"></script>
  <script src="js/app.js"></script>
  <script src="js/controller.js"></script>
  <script src="js/services.js"></script>
  <script src="http://localhost:3000/socket.io/socket.io.js"></script>
  <script type="text/javascript"></script>
</head>
  <body ng-controller="infoCtrl">
  <body>
    <h1><%= titulo %></h1>
    <p><%= info %></p>
  </body>
  <div>
    <input class="form-control" row="15" placeholder="  Filtrar " ng-model="searchText">  
    <hr>
    <div collapse="isCollapsed">
        <div class="col-sm-12">
            <progressbar max="max" value="dynamic"></progressbar>
        </div>
    </div>
    <div collapse="isCollapsedTwo">
    <table id="searchTextResults" class="table table-striped">
      <thead>
        <tr >
          <th class="info">Ubicacion</th>
          <th class="info">Estado</th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat="info in informacion | filter:searchText">
          <td class="{{info.alert}}">{{info.ubicacion}}</td>
          <td class="{{info.alert}}">{{info.estado}}</td>
        </tr>
      </tbody>
    </table>
    </div>
  </div>
</body>
</html>